{
  "name": "seisplotjs-miniseed",
  "version": "1.1.1-SNAPSHOT",
  "description": "Javascript library for parsing seismic data in miniseed format.",
  "main": "./lib/miniseed.js",
  "dependencies": {
    "seisplotjs-model": "^1.0.1",
    "seisplotjs-seedcodec": "^1.0.1"
  },
  "scripts": {
    "lint": "eslint 'src/*.js'",
    "test": "qunit -c ./lib/miniseed.js -t ./test/index.js",
    "compile": "babel --presets es2015 -d lib/ src/",
    "standalone": "browserify src/miniseed.js --standalone seisplotjs_miniseed -o seisplotjs_miniseed_11s_standalone.js -t [ babelify --presets [ es2015 ] ]",
    "prepublish": "npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/crotwell/seisplotjs-miniseed.git"
  },
  "keywords": [
    "seismology",
    "miniseed",
    "steim1",
    "steim2"
  ],
  "author": "Philip Crotwell <crotwell@seis.sc.edu>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/crotwell/seisplotjs-miniseed/issues"
  },
  "homepage": "https://github.com/crotwell/seisplotjs-miniseed",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^7.3.0",
    "eslint": "^4.4.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "qunit": "^1.0.0"
  }
}
